{
  "name": "customer",
  "private": true,
  "scripts": {
    "start": "PORT=8001 umi dev",
    "build": "umi build",
    "lint": "eslint --ext .js .",
    "slint": "stylelint src/**/*.less",
    "release": "standard-version --skip.commit --skip.tag --skip.changelog",
    "changelog": "npm run release && conventional-changelog -p angular -i CHANGELOG.md -w -s -r 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.17"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-qiankun": "^2.10.0",
    "@umijs/preset-react": "1.x",
    "lint-staged": "^10.0.7",
    "yorkie": "^2.0.0",
    "typescript": "^4.1.2",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@commitlint/prompt-cli": "^12.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "husky": "^4.3.0",
    "standard-version": "^9.3.0",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^19.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "mockjs": "^1.1.0"
  }
}
